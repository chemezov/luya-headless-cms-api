{
	"name" : "luyadev/luya-headless-cms",
	"description" : "The bridge between your SPA application and the LUYA CMS Module.",
	"type" : "luya-module",
	"keywords" : ["luya", "headless", "cms", "api", "bridge"],
	"license": "MIT",
	"authors" : [
		{
			"name": "Basil",
			"email": "git@nadar.io"
		}
	],
	"require-dev" : {
	    "luyadev/luya-testsuite" : "^2.0"
	},
	"autoload" : {
		"psr-4" : {
			"luya\\headless\\cms\\" : "src/"
		}
	},
	"autoload-dev" : {
       "psr-4" : {
           "luya\\headless\\cms\\tests\\" : "tests/"
       }
	},
	
    "config": {
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
